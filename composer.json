{
  "name": "estratos/sie-banxico",
  "type": "symfony-bundle",
  "description": "Banxico SIE REST API Client for Symfony Framework",
  "keywords": ["php", "symfony", "banxico sie", "exchange rate"],
  "license": "MIT",
  "authors": [
    {
      "name": "Julio Rodriguez",
      "email": "julio.stratos@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": { "Estratos\\SieBanxico\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": {
      "Estratos\\SieBanxico\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "symfony/http-kernel": "^4.0",
    "phpunit/phpunit": "^7.0",
    "phpstan/phpstan": "^0.9.2",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "^3.2",
    "friendsofphp/php-cs-fixer": "^2.10"
  },
  "require": {
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0"
  },

  "repositories": [
    {
        "type": "git",
        "url": "https://github.com/estratos/sie-banxico.git"
    }
}
